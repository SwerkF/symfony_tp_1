{% extends 'base.html.twig' %}

{% block title %} Edit {{modele.nom}} {% endblock %}

{% block body %}

<div class="card mt-3">
    <div class="card-header card-title"><h1>Edit {{modele.nom}}</h1></div>
    <div class="card-body">
        {{ form_start(editform) }}
            {{ form_errors(editform) }}
            {{ form_row(editform.nom) }}
            {{ form_row(editform.prix) }}
            {{ form_row(editform.marque) }}
        {{ form_end(editform) }}
    </div>
</div>
{% endblock %}
